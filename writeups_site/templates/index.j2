{% extends "base.j2" %}

{% from "writeup_preview.j2" import writeup_preview with context %}

{% block content %}
    <div class="posts">
        {% for (writeup, content) in writeups %}
            {{ writeup_preview(writeup, content, url_for('view', slug=writeup.slug)) }}
        {% endfor %}
    </div>
{% endblock %}
